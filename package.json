{
  "name": "encouraging_prayer_server",
  "version": "1.0.0",
  "main": "./0-compiled/server.mjs",
  "scripts": {
    "//": " --- Server --- ",
    "prewebsite": "rimraf website/build",
    "website": "cd ../website && npm run build",
    "postwebsite": "cp -r ../website/build ../server/website",
    "preportal": "rimraf portal/build",
    "portal": "cd ../portal && npm run build",
    "postportal": "cp -r ../portal/build ../server/portal",
    "build:html": "node ./0-compiled/5-scripts/static-pages.mjs",
    "build:version": " node ./0-compiled/5-scripts/version.mjs",
    "prebuild": "rimraf 0-compiled/",
    "build": "tsc",
    "postbuild": "npm run build:version && npm run build:html",
    "open": "start http://localhost:5000",
    "preserve": "npm run build",
    "serve": "node ./0-compiled/server.mjs",
    "server": "npm run serve",
    "start": "rimraf 0-compiled/ && tsc && node ./0-compiled/server.mjs",
    "dev": "aws sso login && npm run start",
    "sync-config": "node ./0-compiled/5-scripts/config-sync.mjs",
    "update": "npm run sync-config && npm run website && npm run portal & npm run postbuild",
    "archive": "./scripts/prayer-request.sh",
    "clean": "rimraf node_modules/ package-lock.json 0-compiled/"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.744.0",
    "@aws-sdk/client-s3": "^3.417.0",
    "@aws-sdk/client-secrets-manager": "^3.565.0",
    "@aws-sdk/client-sns": "^3.709.0",
    "@types/jsonwebtoken": "^9.0.2",
    "argon2": "^0.41.1",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "metascraper": "^5.45.9",
    "metascraper-description": "^5.45.9",
    "metascraper-image": "^5.45.9",
    "metascraper-title": "^5.45.9",
    "mysql2": "3.11.3",
    "node-cron": "^4.2.1",
    "path": "^0.12.7",
    "rimraf": "5.0.10",
    "socket.io": "^4.5.4",
    "stack-trace": "^1.0.0-pre1",
    "ts-node": "9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.5",
    "typescript": "^4.8.4"
  }
}
